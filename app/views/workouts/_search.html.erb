<%= form_tag workouts_path, :class => 'form-inline', method: 'get' do %>
  <div class="form-group">
  	<%= text_field_tag 'search', @search, :class => "form-control" %>
  	<%= submit_tag 'Search', :class => 'btn button_grey' %>  <%= link_to "View All Workouts", workouts_path(id: "all"), :class => 'btn button_grey' %>
  </div>
<% end %>